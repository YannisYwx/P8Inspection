<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/v_right"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.65" />

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_partner"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_order_time"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_order_number"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_account_time"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_wechat"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_status"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_parking_duration"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_parking_number"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_money"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_partner_shared"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>


    <View
        android:layout_width="0dp"
        android:layout_height="2.5pt"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_description"
        android:layout_marginStart="60pt"
        android:layout_marginEnd="60pt"
        android:background="@color/window_bg"/>

    <TextView
        android:id="@+id/tv_order_time"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_order_time"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_order_time" />


    <TextView
        android:id="@+id/tv_order_number"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_order_number"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_order_number" />

    <TextView
        android:id="@+id/tv_parking_number"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_parking_number"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_parking_number" />

    <TextView
        android:id="@+id/tv_account_time"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_account_time"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_account_time" />

    <TextView
        android:id="@+id/tv_duration"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_parking_duration"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_parking_duration" />

    <TextView
        android:id="@+id/tv_status"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_status"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_status" />

    <TextView
        android:id="@+id/tv_wechat"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_wechat"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_wechat" />

    <TextView
        android:id="@+id/tv_money"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_money"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_money" />

    <TextView
        android:id="@+id/tv_partner_shared"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_partner_shared"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_partner_shared" />

    <TextView
        android:id="@+id/tv_description"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_description"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_description" />

    <TextView
        android:id="@+id/tv_partner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="80pt"
        android:layout_marginEnd="150pt"
        android:textColor="@color/text_black"
        android:textSize="@dimen/item_label_text_size"
        app:layout_constraintBottom_toBottomOf="@id/tv_label_partner"
        app:layout_constraintLeft_toLeftOf="@id/v_right"
        app:layout_constraintRight_toRightOf="@id/v_right"
        app:layout_constraintTop_toTopOf="@id/tv_label_partner" />

    <TextView
        android:id="@+id/tv_label_order_number"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="订单流水号："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_partner"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_parking_number"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="车位号："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_order_number"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_order_time"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="下单时间："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_parking_number"
        app:layout_constraintWidth_percent="0.35" />


    <TextView
        android:id="@+id/tv_label_account_time"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="结算时间："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_order_time"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_parking_duration"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="停车时长（分）："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_account_time"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_partner"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="合作机构："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_status"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="状态："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_parking_duration"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_wechat"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="微信号："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_status"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_money"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="支付金额（元）："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_wechat"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_partner_shared"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="合作机构分成："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_money"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/tv_label_description"
        style="@style/TextLabel"
        android:layout_width="0dp"
        android:text="描述："
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_label_partner_shared"
        app:layout_constraintWidth_percent="0.35" />


</androidx.constraintlayout.widget.ConstraintLayout>